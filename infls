check_rootsf(Root):-
  on(Root, [flett, riflett, deflett, succed, perd]),!.

pfd(1, b, esti,     indic,  pass,  2, s).
pfd(1, b, este,   indic,  pass,  2, p).
pfd(1, b, emmo,     indic,  pass,        1, p).

pfd(1, b, uto, part, pass, m, s).
pfd(1, b, uta, part, pass, f, s).
pfd(1, b, uti, part, pass, m, p).
pfd(1, b, ute, part, pass, f, p).

pfd(1, c, iuta,     part,   pass,  f,  s).
pfd(1, c, iute,     part,   pass,  f,  p).
pfd(1, c, iuti,     part,   pass,  m,  p).
pfd(1, c, iuto,     part,   pass,  m,  s).

pfd(1, c, esti,     indic,  pass,  2, s).
pfd(1, c, este,   indic,  pass,  2, p).
pfd(1, c, emmo,     indic,  pass,        1, p).

pfd(1, e, ite,  imp,   pres,  2, p).
pfd(1, e, iamo,  imp,   pres,  1, p).

pfd(1, f, ete,  imp,   pres,  2, p).
pfd(1, f, iamo,  imp,   pres,  1, p).

pfd(1, g, Fles,  Mod, Tem, P, N):-
   (Mod=indic, Temp=fut; Mod=cond, Tem=pres),
   pfd(6, a, Fles,  Mod, Tem, P, N).

pfd(1, g, Fles,  Mod, Tem, P, N):-
   (Mod=subj, Temp=pres
    ; 
    Mod=indic, Temp=pres
    ; 
    Mod=imp, Temp=pres
    ; 
    Mod=indic, Temp=pass
    ; 
    Mod=subj, Tem=imperf),
   pfd(3, g, Fles,  Mod, Tem, P, N).


pfd(2, b, ete,     indic,  pres,  2, p). /*solere*/

