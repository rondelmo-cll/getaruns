:- style_check(-singleton).

search_suffix(L,List,Root-Feats):-
     ssuf(List,W1,Feats,Code,Root),
     !.
     
get_root(W,Feats,Root-Subs):-
     stringof(List, W),
     ssuf(List,W1,Feats,Code,Roo),
     extrroot(Code,Roo,Root),
     findall(Sub,v(Root,Cat,Con,Sub),Subs),
     Subs\=[],
     !.

get_root_clit(W,Feats,Root-Subs,Clit):-
     stringof(List, W),
     ssuf(List,W1,Feats,Code,Roo),
     extrroot(Code,Roo,Root),
     (on(encl=Cl,Code),Clit=Cl;Clit=nil),
     findall(Sub,v(Root,Cat,Con,Sub),Subs),
     Subs\=[],
     !.

extrroot(Code,Root,Root):-
     nonvar(Root),
     !.
extrroot(Code,Roo,Root):-
    var(Roo),
    \+ on(encl=B,Code),
    \+ on(vf=D,Code),
    on(C,Code),
    concat_atom(X,:,C),
    length(X,L),
    (L=4,
    X=[Ca,S,Root|_]
    ;
    L=3,
    X=[Ca,Root|_]),    
    Ca=v, \+ integer(Root),
     !.
    
extrroot(Code,Roo,Root):-
    var(Roo),
    (on(encl=B,Code)
     ;on(vf=D,Code)),
    on(C,Code),
    term_to_atom(C,C1),
    concat_atom(X,:,C1),
    length(X,L),
    L=3,
    X=[Ca,Roots|_],
    Ca=v,
    stringof(Roo,Roots),
    (\+ on('-',Roo),Root=Roots
     ;
     on('-',Roo),
     concat_atom(X1,-,Roots),
     length(X1,L1), L1=5,
     reverse(X1,[Root|_])
     ),
    !.
     
pre_leaf(PhraseIn, Code, NRoot) :-
   nonvar(PhraseIn),
   pref_name(PhraseIn, Pre+Root),
   stringof(String, Root),
   length(String, L),
   1 < L,
   (p_lex(Root, Code, All), Morpho=Root
    ;
   ana_morpho(Rest, String, TopDown, Morpho, Code, NRoot, Infl)).

spre_parse(cw(_, Feat), Pre, _):-
   is_pref(Pre, Code),
   (Code=el1;Code=el1el2),
   !.
is_pref(Pre, Code) :-
   p(Pre, Codes),
   sort(Codes, SortCodes), 
   on(CodeAtm, SortCodes),
%   pname(Code, CodeAtm),
   is_pr(Pre, CodeAtm).

is_pr(Pre, pre).
is_pr(Pre, pred).
is_pr(Pre, pref).
is_pr(Pre, pren).
is_pr(Pre, prev).
is_pr(Pre, el1).
is_pr(Pre, el1el2).
   
choose_p(an).
choose_p(bi).
choose_p(co).
choose_p(di).
choose_p(de).
choose_p(es).
choose_p(eu).
choose_p(in).
choose_p(oo).
choose_p(ra).
choose_p(re).
choose_p(ri).
choose_p(si).
choose_p(so).

p_lex(W, Tag, Type) :-
    s(W,Tag,F),
     !.
p_lex(W, Tag, Type) :-
    l(W,Tag),
     !.
p_lex(W, Tag, Type) :-
    t(W,Tag),
     !.
p_lex(W, Tag, Type) :-
    de(W,Tag),
     !.
p_lex(W, Tag, tml) :-
    tml(W,Tag),
     !.
p_lex(W, np, Type) :-
    pw(Words,W),
     !.
p_lex(W, nh, Type) :-
    toupper(W,W1),
    nmi(W1),
     !.
p_lex(W, nh, Type) :-
    toupper(W,W1),
    nfi(W1),
     !.
p_lex(W, np, Type) :-
    toupper(W,W1),
    up(W1),
     !.
p_lex(W, Tag, Type) :-
    wl(W,Tag,F,Gen),
     !.
p_lex(W, avv, Type) :-
    avvs(W),
     !.
/*
p_lex(W, Tag, Type) :-
    mlt(Pol,W,Tag),!.
*/    
getrootword(Word,Roo,Root):-
   var(Word),
   (Roo \= A+B, Root=Roo
     ;
     Roo=A+B,
    concat(A,B,Root)),
   !.
getrootword(Word,Roo,Root):-
   nonvar(Word),
   (Word \= A+B, Root=Word
     ;
     Word=A+B,
    concat(A,B,Root)),
   !.
getrootword(Word,Roo,Root):-
    nonvar(Roo),
    Root=Roo,
   !.
  
ssuf(Revl,Word,Cat,Code,Roo):-
    findall(Code-Root,ana_morpho(Rest, Revl, Mid, Word, Code, Root, Infl),Codes),
    Codes\=[], on(Code-Root,Codes),
    (is_list(Code), 
         (Code=[A],Catall=[A];
          Code\=[A],
          (
             findall(Cats,(on(Cod,Code),stringof(List,Cod),
                           \+ member('=',List),
                           term_to_atom(Codd,Cod),
                           sellexcl(Codd,Cats)), Catt), 
                           sort(Catt,Catr),appiattisci(Catr,Catall),
                           Catall\=[]
              ;
             findall(Cats,(on(Cod,Code),
                           sellexcl(Cod,Cats)), Catt), 
                           sort(Catt,Catr),appiattisci(Catr,Catall),
                           Catall\=[]
              )),
    extractcat(Catall,Cat)
         ;
     atomic(Code), Cat=Code),
     getrootword(Word,Root,Roo).

ssuf(Revl,Word,Tags,Code,Root):-
    anamorpho(Rest, Revl, Mid, Word, Tags, Code).
%    extractcat(Code,Tags).

ssuf(Revl,Word,Tags,Code,Root):-
    pre_leaf(Revl, Code, Word),
    extractcat(Code,Tags),
    !.

ssuf(Revl,Word,[Cat],Code,Root):-
    suf_nums(Revl, Feats),
    (
     on(type=pro:N,Feats), Cat=n;
     Cat=num),
    !.
        

/************* • 
ana_morpho(
   +wordAtom, +wordString, +restIn, +topDown
   -morphoAtom, -code, -restOut). •• 
*************/

/* 4 */
ana_morpho(Morph, MorphoStr, Rest, Morphos, NCode, Morphor, RestOut+Cli) 
   :-
   stringof(Clitsuf, Rest),
    length(Clitsuf, N),
   look_ahead3(N, TopDown, Rest, ''),
   stringof(MorphoStr, NewMorpho),
   (ana_cli(Rest, NewMorpho, MorphoStr, '', Cat, Morpho, Code, NCli),
    define_cat(Cat, CCat),
    stringof(MorphoSt, Morpho), Morphon=Morpho
    ;
    MorphoSt=MorphoStr,
    Morphon=NewMorpho,
    NCli=Rest,
    Cli=Rest,
    en_cli(Cli, ['clit:proenc']),
    CCat='clit:proenc'
   ),
   (search_inter(Morphon, Cli, Morpho, NCode, RestOut),!
    ;
    nonrecursive(Morphon, MorphoSt, NCli, Morphor, NCode, CCat, RestOut+Cli)
    ).


ana_morpho(Morpho, MorphoStr, TopDown, Morphos, Tg, NMorpho, TopDown) :-
   nonvar(TopDown),
   slook_ahead(TopDown, Code),
   stringof(Clitsuf, TopDown),
   length(Clitsuf, N),
   lex_lookup(N, Code, Ds, Morpho, NMorpho, Tags),
   (on(av,Code), Tg=Code;Tg=Tags).
   
/* 1 */
ana_morpho(Morpho, MorphoStr, TopDown, Rest, Code, NMorpho, Rest) :-
   var(TopDown),
   nonvar(MorphoStr),
   stringof(MorphoStr,Clitsuf),
   look_ahead2(N, Ds, Clitsuf, Rest),
   lex_lookup(N, nil, Ds, Clitsuf, NMorpho, Code).

/* 2 */
ana_morpho(Morph, MorphoStr, TopDown, Rest, Code, Root, Rest1) :-
   nonvar(TopDown),
   stringof(Clitsuf, TopDown),
   length(Clitsuf, N),
   look_ahead4(N, TopDown, Rest, Root, Rest1),
   mcon(Morph, Root, NewRoot),
   lex_lookup(N, nil, _, NewRoot, NewRoot, Code).

ana_morpho(Morph, MorphoStr, TopDown, Morphos, Code, NewRoot, Rest1) :-
   nonvar(TopDown),
   stringof(Clitsuf, TopDown),
   length(Clitsuf, N),
   look_ahead4(N, Rest, TopDown, Root, Rest1),
   mcon(Morph, Root, NewRoot),
   lex_lookup(N, nil, _, NewRoot, NewRoot, Code).


ana_morpho(Morpho, MorphoStr, TopDown, Rest, Codes, NMorpho, TopDown) :-
   nonvar(TopDown),
   nonvar(Morpho),
   stringof(Clitsuf, TopDown),
   length(Clitsuf, N),
   pro_lex(N, Ds, Morpho, NMorpho, Code), Code\=[],
   matchcheckcats(Types,Code,Codes).

/* 3
ana_morpho(Morpho, MorphoStr, TopDown, Rest, Code, Morpho, Rest) :-
   nonvar(TopDown),
   stringof(Clitsuf, TopDown),
   length(Clitsuf, N),
   look_ahead2(N, Ds, TopDown, Rest),
   lex_lookup(N, nil, Ds, Morpho, NMorpho, Code).

ana_morpho(Morph, MorphoStr, mente, TopDown, Morph1, SCode, Rest1) :-
    stringof(MorphoStr, Morpho),
    ana_morpho(Morpho, MorphoStr, '', TopDown, Morphos, SCode, Rest),
    Rest=issima,
    Rest1=issima+mente,
    Morph1=Morphos.

ana_morpho(Morpho, MorphoStr, TopDown, Morphos, Code, NewRoot, Rest1) :-
   nonvar(TopDown),
   stringof(Clitsuf, TopDown),
   length(Clitsuf, N),
   look_ahead4(N, H, TopDown, Root, Rest1),
   mcon(Morpho, Root, NewRoot),
   lex_lookup(N, nil, _, NewRoot, NewRoot, Code).
 */

ana_morpho(_, String, RestIn, Morpho, Code, Root, RestOut) :-
   append(Prefix,  [Car], String),
   stringof(Prefix, Word),
   (var(RestIn),RestMid=Car;
    nonvar(RestIn),
   concat(Car, RestIn, RestMid)),
   stringof(Pr, RestMid),
   length(Pr, N),
   N < 10,
   ana_morpho(Word, Prefix, RestMid, Morpho, Code, Root, RestOut).

anamorpho(Morpho, MorphoStr, TopDown, Morph, Codes, Tag) :-
   nonvar(TopDown),
   lookahead(TopDown, Code),
   dr(Morpho,Tag),
   extractcat(Code,Tags),
   extractcat(Tag,T),
   (subtract(T,[C],Res),subtract(Tags,[C],Code), Codes=C
   ;
   Tags\=[], Codes=Tags
   ;
   Codes=T),
   !.
anamorpho(_, String, RestIn, Morpho, Code, Tag) :-
   append(Prefix,  [Car], String),
   stringof(Prefix, Word),
   (var(RestIn),RestMid=Car;
    nonvar(RestIn),
   concat(Car, RestIn, RestMid)),
   stringof(Pr, RestMid),
   length(Pr, N),
   N < 10,
   anamorpho(Word, Prefix, RestMid, Morpho, Code, Tag).

slook_ahead(Flex, Ts):-
      plx(L,Flex, Tag),
      findall(T,
           (member(dv,Tag),T=v;
            member(dn,Tag),T=n;
            member(dadv,Tag),T=av;
             member(dadj,Tag),T=ag), Ts),!.

slook_ahead(Flex, [v]):-
     fd(Cl, Ty, Con, Flex,     Mode,   Temp,  G,  N),
     !.
     
look(Flex, T):-
      clx(L,Flex, Tag),
      (member('clit:proenc',Tag);member('clit:enc',Tag);
        member('clit:pro',Tag);member('clit:double',Tag);
         member('clit:r_enc',Tag)),
     !.

lookahead(TopDown, Code):-
    sf(TopDown, Code),
    !.
