    chappend(Tagg,Tags,Tag),
     !.
*/
ttgtext(N,W,Pol,Low,W-Tag):-
    s(W,Tag,F),
     !.
ttgtext(N,W,1,Low,W1-Tag):-
    (N=1;N=0;fct(Low)),
    s(Low,Tag,F),W1=Low
     ;
     Low\=W, 
     if_first_name(W),
     Tag=nh,W1=W,
     !.
ttgtext(N,W,Pol,Low,W-Tags):-
    Pol=0,
    tml(W,Tag),
    extractcat(Tag,Tags),
     !.
ttgtext(1,W,1,Low,W-Tag):-
    uneag(Low,Tags),
    wmapseag(Low,Tag,Tags),
     !.
ttgtext(N,W,Pol,Low,W-Tag):-
%   \+ mbw(W,_),
    wl(W,Tag,F,Gen),
     !.
ttgtext(N,W,1,Low,W-Tag):-
    (N=1;N=0),
    wl(Low,Tag,F,Gen),
     !.
ttgtext(N,W,Pol,Low,W-Tag):-
     allcatsde(W,Tag),
     !.
ttgtext(N,W,Pol,Low,W-Tag):-
    uneag(W,Tags),
    wmapseag(Low,Tag,Tags),
     !.
ttgtext(N,W,Pol,Low,W-Cat):-
    \+fct(Low),
    (
    exploresemantic(W,Cat)
    ;
    exploresemantic(Low,Cat)),
    !.
ttgtext(N,W,Pol,Low,W-Tags):-
    vlm(W,Tag,F),
    checknominals(W,Pol,Low,Tags),
     !.
ttgtext(N,W,Pol,Low,W-Tag):-
    g(W,Tag,F),
     !.
ttgtext(N,W,Pol,Low,W-Tag):-
     allcatsili(W,Tag),
     !.
ttgtext(N,W,Pol,Low,W-Tag):-
    stringof(List,W),
    List=[#|Rest],
    stringof(Rest,W1),
    ttgtext(N,W1,Pol,Low,W1-Tag),
    !.
ttgtext(N,W,Pol,Low,Low-[intj]):-
    stringof(List,Low),is_list(List),
    findall(h,(member(h,List),
               (member(a,List);member(e,List))
               ),H),
    length(List,L),4<L,
    length(H,L1),4<L1,
     !.
ttgtext(N,W,Pol,Low,Low-[intj]):-
    stringof(List,Low),is_list(List),
    length(List,L),3=<L,
    member('!',List),
     !.
ttgtext(N,W,Pol,Low,W-[T]):-
    \+fct(Low),
    lw(W,T,Def),
    !.
ttgtext(N,W,Pol,Low,W-[Tag]):-
    \+fct(Low),
    sst(Low,Cat,Sem),
    decodecatsst(Pol,Cat,Sem,Tag),
    !.
