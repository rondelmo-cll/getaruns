    stringof(List,W),
    length(List,L), 3=<L,
    p(W,_),
     !.
ttgtext(N,W,Pol,Low,W-Tag):-
    stringof(List,W),
    length(List,L), 4=<L,
    checklocsall(L,W,Tag),
    !.
