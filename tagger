translatecats(s,S,n).
translatecats(sa,S,ag).
translatecats(ppres,S,[ag,n]).
translatecats(loc,S,[av,php]).
translatecats(loc,S,av).
translatecats(slash,S,punt).
translatecats(a,num,num).
translatecats(pron,int,int).
translatecats(pron,S,S):-
   \+ integer(S),!.
translatecats(pron,S,Cat):-
   \+ integer(S),
   mcon(pron,'_',C),
   mcon(C,S,Cat).
translatecats(Ca,S,Ca).

