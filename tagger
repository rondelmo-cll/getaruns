      disambche(W-[]-punto-1,che-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,Mine0-N0),
      deletezeros([W2-Distr2-Mine2-N2|Disouts],Diss),
      !.
deletezeross(P,[W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2|Disouts],[W1-Distr1-Art-0|Diss]):-
      \+ indefs(W1),
      (atomic(Mine1), Mine1=art;is_list(Mine1),on(art,Mine1),Art=art;
       atomic(Mine1), Mine1=deit;is_list(Mine1),on(deit,Mine1),Art=dim),
      (N1=1, 
         (on(adj,Distr1);on(n,Distr1))
        ;N1\=1),
      (atomic(Mine2), Mine2=ag;is_list(Mine2),on(ag,Mine2)),
      (detectagn(W2,Mine0,N,Disouts)
      ;
       subtract(Mine2,[cong],Min1),
       subtract(Min1,[v],Mine0)),
      deletezeros([W2-Distr2-Mine2-N2|Disouts],Diss),
      !.
      
