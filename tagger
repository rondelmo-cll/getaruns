computefssbest(W,Tags):-
   \+fct(W),
   allfs2(W,Cat,Allf),
   sort(Allf,Alls),
   getallfreqtags(Alls,Tags),
   !.
computefssbest(W,Tags):-
%   \+ mbw(W,_),
   allfs1(W,Cat,Allf),
   sort(Allf,Alls),
   getallfreqtags(Alls,Tags),
%   selectreastag(Alls,Tags,Tag),
%   Tag\=[],
   !.
*/


selectreastag(Alls,Tags,Tag):-
   reverse(Alls,RevAl),
   RevAl=[F1-Ca|_],
   (islist(Ca),Ca=[C];atomic(Ca),C=Ca),
   (
   1000<F1, F2=12;
    100<F1, F2=6;
     10<F1, F2=3; 
    F2=1),
   (remove(F-T,Alls,Rest), F=<F2, T\=C,
    remove(T,Tags,Tag)
    ;
    Tag=Tags
    ),
   !.

getallfreqtags(Alls,Tags):-
   getallftags(Alls,Taggs),
   appiattisci(Taggs,Tas),
   sort(Tas,Tags),
   !.
getallftags([],[]):-!.
getallftags([_-T|Alls],[T|Tags]):-
   getallftags(Alls,Tags),
   !.

ttagtext(N,[],[]):-!.

ttagtext(1,['"'|Text],['"'-[par]|Out]):-
    ttagtext(1,Text,Out),
     !.

ttagtext(1,[W|Text],[Tag|Out]):-
    atomic(W),
    spy_lower(W, Low, Pol),
    ttgtext(1,W,Pol,Low,Tag),
    assert(firstword(W)),
    ttagtext(2,Text,Out),
     !.
ttagtext(N,[W|Text],[Tag|Out]):-
    atomic(W),
    spy_lower(W, Low, Pol),
    ttgtext(N,W,Pol,Low,Tag),
    N1 is N + 1,
    ttagtext(N1,Text,Out),
     !.
ttagtext(N,[W|Text],[Tag|Out]):-
    ttgtext(N,W,Pol,W,Tag),
    N1 is N + 1,
    ttagtext(N1,Text,Out),
     !.

getnumfile(Sent,Num):-
    stringof(List,Sent),
    reverse(List,Rev),
    getnf(Rev,Num),
    !.

getnf([A,B,C,N|Rev],Num):-
    integer(N),
    integer(C),
    integer(B),
    integer(A),
    stringof([N,C,B,A],Num),
    !.
getnf([A,B,N|Rev],Num):-
    integer(N),
    integer(B),
    integer(A),
    stringof([N,B,A],Num),
    !.
getnf([A,N|Rev],Num):-
    integer(N),
    stringof([N,A],Num),
    !.
getnf([N|Rev],N):-
    integer(N),
    !.

alltoupper(W,W2):-
     stringof(List,W),
     toupperlist(List,ListW2),
     stringof(ListW2,W2).
alltoupper(W,W2):-
     recover_descs(W,Desc),is_list(Desc),
     Desc=[A,B],
     mcon(A,'_',A1),
     mcon(A1,B,W2),
     !.
toupperlist([],[]):-!.
toupperlist([W|List],[W1|ListW2]):-
     toupper(W,W1),
     toupperlist(List,ListW2),
     !.
    
dialttagtext(Sent,N,[],[]):-!.
dialttagtext(Sent,1,[W,':'|Text],[Tag,dice-vt,':'-[dirs]|Out]):-
    atomic(W),
    spy_lower(W, Low, Pol),
    (stringof(List,W),member('-',List),
%     recover_descs(W,Descs),
     concat_atom(Descs,'-',W),
     concat_atom(Descs,'_',W1)
     ;
     W1=W),
    ttgtext(1,W1,Pol,Low,Tags),
    (Tags\=[nw],Tag=Tags;
      Descs=[W2|_],
      ttgtext(1,W2,Pol,Low,Tag)
       ),    
    getnumfile(Sent,Num),
    asserta(speaker(Num,W1)),
    dialttagtext(Sent,1,Text,Out),
     !.
dialttagtext(Sent,1,[W|Text],[Tag|Out]):-
    atomic(W),
    spy_lower(W, Low, Pol),
    ttgtext(1,Low,Pol,Low,Tag),
    assert(firstword(W)),
    dialttagtext(Sent,2,Text,Out),
     !.
% @andreaiannone29
dialttagtext(Sent,1,[W|Text],[W-[nh]|Out]):-
    atomic(W),
    stringof(List,W),List=['@'|Rest],
    stringof(Rest,W1),
    getnumfile(Sent,Num),
    asserta(speaker(Num,W1)),
    dialttagtext(Sent,2,Text,Out),
     !.
dialttagtext(Sent,3,[W|Text],[W-Tag|Out]):-
    select_punct(W, Tag),
    dialttagtext(Sent,3,Text,Out),
     !.
dialttagtext(Sent,N,[W|Text],[Tag|Out]):-
    atomic(W),
    (W=x,W1=per;W=xké,W1=perché;
     stringof(List,W),
     (List=['#'|Rest],stringof(Rest,W1);
      List=['@'|Rest],stringof(Rest,W1))
     ),
    ttgtext(N,W1,0,W1,Tag),
    N1 is N + 1,
    dialttagtext(Sent,N1,Text,Out),
     !.
dialttagtext(Sent,N,[Punt,W|Text],[Punt-K,Tag|Out]):-
    select_punct(Punt,K),
    (K=[punto];K=[puntint]),
    atomic(W),
    spy_lower(W, Low, Pol),
    ttgtext(1,W,Pol,Low,Tag),
    dialttagtext(Sent,2,Text,Out),
     !.
dialttagtext(Sent,N,[W|Text],[Tag|Out]):-
    atomic(W),
    spy_lower(W, Low, Pol),
    ttgtext(N,W,Pol,Low,Tag),
    N1 is N + 1,
    dialttagtext(Sent,N1,Text,Out),
     !.
dialttagtext(Sent,N,[W|Text],[Tag|Out]):-
    ttgtext(N,W,Pol,W,Tag),
    N1 is N + 1,
    dialttagtext(Sent,N1,Text,Out),
     !.

chappend(Tagg,Tags,Tag):-
    atomic(Tagg),atomic(Tags),
    Tag=[Tagg,Tags],
    !.
chappend(Tagg,Tags,Tag):-
    atomic(Tagg),is_list(Tags),
    append([Tagg],Tags,Tag),
    !.
chappend(Tagg,Tags,Tag):-
    atomic(Tags),is_list(Tagg),
    append(Tagg,[Tags],Tag),
    !.
chappend(Tagg,Tags,Tag):-
    is_list(Tags),is_list(Tagg),
    append(Tagg,Tags,Tag),
    !.
    
ttgtext(N,[],_,_,[]):-!.
ttgtext(N,W,Pol,Low,W-Tag):-
     Pol=1, 1 < N, \+preposs(Low),
     if_first_name(W),Tag=nh,
     !.
ttgtext(N,W,Pol,Low,W-Tag):-
      preparts(Low),
      selectdi(Low),Tag=partd
      ;
      preparts(Low),Tag=part
      ;
      selectdi(Low),Tags=pd,
      (1<N,Pol=1,Tag=nh;Tag=pd)
      ;
      preposs(Low),Tag=p
      ;
      ppol(Low-T),Tag=php
      ;
      ppv(Low-T),
       matchdistrs(T,Tag),
       (Tag=php;Tag=p;Tag=part)
      ;
      agp(Low-T),mapseag(Tag,T),
       (Tag=php;Tag=p;Tag=part),
     !.
ttgtext(N,W,Pol,Low,W-Tag):-
      preps(Low,Tag,_,_),
      !.
ttgtext(N,W,Pol,Low,W-poss):-
     uneag(Low, adj_pos),
     !.
