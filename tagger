      Mine=[ag,n],
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      nouncats(B),
      wnit(W,n,Code),
      disamb2([W1-Distr1-Mine1-1|Disouts],Diss),
      !.            
disamb2([W-Distr-Mine-2,W1-Distr1-Mine1-0|Disouts],Diss):-
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      findall(Mine0,
                 (on(A,Distr),
                   filtercats(A,W,B,W1,A1,B1),
                     collecttrim2(Mine,B1,Mine0)), Trimms),
      appiattisci(Trimms,Tris),sort(Tris,Trims),
      evaluatetrims2(Trims,Mine0),
      Mine0\=[],
      disamb2([W-Distr-Mine0-1,W1-Distr1-Mine1-0|Disouts],Diss),
      !.      

disamb2([W-Distr-Mine-N|Disouts],[W-Distr-Mine-N|Diss]):-
      disamb2(Disouts,Diss),
      !.

