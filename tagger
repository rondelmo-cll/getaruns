ttgtext(N,W,Pol,Low,W-[Cat]):-
    \+fct(Low),
    (jrc([W],Code,Nu);
     jrc([A,W],Code,Nu);
     jrc([A,_,W],Code,Nu);
     jrc([A,_,_,W],Code,Nu)),
    (Code=pers,Cat=nh;Cat=npro),
    !.
