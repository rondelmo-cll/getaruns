/* TAGSET
abbr	=	abbreviations
ag	=	adjective
aggpp = superlativo			
agn	=	nominal adjective or past participle
art	=	article def/indef		
auag  	=	auxiliary "avere" gerundive
auair	=	auxiliary "avere" mood irrealis  
aueg  	=	auxiliary "essere" gerundive
aueir	=	auxiliary "essere" mood irrealis  
ausa  	=	auxiliary "avere" tensed
ausai  	= 	auxiliary "avere" infinitival	
ause	=	auxiliary "essere" tensed   
ausei    	=   	auxiliary "essere" infinitival
ausep    	=	auxiliary "essere" past participle  
avv	=	adverb
avvl	=	adverbial locution	
ccom  	= 	conjunction "come" comparative
ccong	=	conjunction comparative subordinate
clit	=	clitic pronoun
clitabl  	=	clitic pronoun ablative(locative)
clitac	=	clitic pronoun accusative	
clitdat  	=	clitic pronoun dative
cong	=	conjunction
congf	=	conjunction sentential
conjl	=	conjunction locution	
cosu	=	conjunction subordinate
cosul	=	conjunction subordinate locution
da_riempire 	= dots ...
date	=	date number
deit	=	deictic pronoun 		
dim	=	demonstrative adjective
dirs	=	cue direct/indirect discourse ( : )  
equal	=	segno di uguale
exc	=	exclamation pronoun	
fw	=	foreign word = noun	
in	=	intensifier
ind	=	adjective indef		
int	=	interrogative pronoun
intj	=	interjection		
n	=	noun			
nc	=	noun colour
neg	=	negation
nf	=	noun factive		
nh	=	noun human
np	=	noun proper geographic/institution
npro	=	proper noun for upper case ovw(out of vocabulary words)
nt	=	noun temporal	
num	=	numeral
p	=	preposition		
par  	=	parenthetical ( '(' ')' '"') -
part	=	preposition plus article
partd	=	preposition "di" plus article	
partda	=	preposition "da" plus article
pd	=	preposition "di"		
pda	=	preposition "da"
php	=	preposition locution	         
pk	=	complementizer		  
poss	=	possessive adjective
ppas  	=  	past participle absolute
ppre	=	present participle absolute	
pron	=	pronoun			
pt	=	verbal particle
punt  	=  	sentence internal (, - = § )
puntint  	=	punctuation non declarative ( ? ! )
punto	=	sentence final (. ;)	
q	=	quantifier			
qc	=	quantifier collective 
qd	=	quantifier distributive	
rel	=	relative 
relin	=	relative indefinite	
relob	=	relative oblique		
relq	=	relative quantifier
sect	=	lettera o numero indicatore di sezione
slash	=	segno "/"
titl	=	titolo
vc	=	verb copulative		  
vci	=	verb copulative infinitive
vcir   	=  	verb copulative mood irrealis
vcl	=	verbo con enclitico
vgc	=	verb copulative gerundive
vgin	=	verb intrans gerundive	  
vgprog  	=  	verbo progressivo gerundio
vgs	=	verb modal gerundive	  
vgsf   	=	verb reconstruction gerundive
vgt	=	verb trans gerundive	
viin	=  	verb intrans infinitival
vin	=	verb intrans tensed		
virin	=  	verb intrans mood irrealis
virt	=	verb trans mood irrealis	  
vit	= 	verb trans infinitive
vppc	=	verb copulative past participle
vppin	=	verb intrans past participle	
vppt	=  	verb trans past participle
vprc	=	verbo copulativo participio presente  
vprin	=	verb intrans present participle  
vprog  	=	verb progressive
vprogir	=	verb progressive mood irrealis  
vprt  	= 	 verb trans present participle
vsf	=	verb "fare" periphrastic
vsup  	=	verb modal
vsupir	=	verb modal mood irrealis	  
vt	=	verb trans tensed 
*/  
   
   
/********************* TAGGING IMPROVEMENTS *******************/

/*
ADJ
ADV
ART
NN
ADJ_DIM
ADJ_IES
ADJ_IND
ADJ_NUM
ADJ_POS
C_NUM
CONJ_C
CONJ_S
INT
NN_P
NULL
P_OTH
PREP
PREP_A
PRON_DIM
PRON_IES
PRON_IND
PRON_PER
PRON_POS
PRON_REL
V_AVERE
V_CLIT
V_ESSERE
V_GVRB
V_MOD
V_PP
*/
mapseag(abbr,null).
mapseag(ag,adj).
mapseag(agn,adj).
mapseag(art,art).
mapseag(avv,adv).
mapseag(avvl,adv).
mapseag(ccom,conj_c).
mapseag(ccong,conj_c).
mapseag(cong,conj_c).
mapseag(congf,conj_c).
mapseag(conjl,conj_c).
mapseag(clit,pron_pers).
mapseag(clitabl,pron_pers).
mapseag(clitac,pron_pers).
mapseag(clitdat,pron_pers).
mapseag(cosu,conj_s).
mapseag(cosul,conj_s).
mapseag(date,c_num).
mapseag(deit,pron_dim).
mapseag(dim,adj_dim).
mapseag(exc,int).
mapseag(in,adv).
mapseag(ind,adj_ind).
mapseag(intj,null).
mapseag(n,nn).
mapseag(nc,nn).
mapseag(nf,nn).
mapseag(neg,adv).
mapseag(nt,nn).
mapseag(nh,nn_p).
mapseag(np,nn_p).
mapseag(npro,nn_p).
mapseag(num,c_num).
mapseag(p,prep).
mapseag(part,prep_a).
mapseag(partd,prep_a).
mapseag(partda,prep_a).
mapseag(pd,prep).
mapseag(pda,prep).
mapseag(php,prep).
mapseag(pk,conj_s).
mapseag(poss,pron_pos).
mapseag(poss,adj_pos).
mapseag(ppas,v_pp).
mapseag(ppre,v_pp).
mapseag(pron,pron_per).
mapseag(pron,pron_ind).
mapseag(pt,prep).
mapseag(q,pron_ind).
mapseag(qc,pron_ind).
mapseag(qd,pron_ind).
mapseag(rel,pron_rel).
mapseag(relin,pron_rel).
mapseag(relob,pron_rel).
mapseag(relq,pron_rel).
mapseag(vcl,v_clit).
mapseag(vgin,v_gvrb).
mapseag(vgprog,v_gvrb).
mapseag(vgs,v_mod).
mapseag(vgsf,v_gvrb).	
mapseag(vgt,v_gvrb).
mapseag(viin,v_gvrb).
mapseag(vin,v_gvrb).
mapseag(virin,v_gvrb).
mapseag(virt,v_gvrb).
mapseag(vit,v_gvrb).
mapseag(vppc,v_pp).
mapseag(vppin,v_pp).
mapseag(vppt,v_pp).
mapseag(vprc,v_pp).
mapseag(vprin,v_pp).
mapseag(vprog,v_gvrb).
mapseag(vprogir,v_gvrb).
mapseag(vprt,v_pp).
mapseag(vsf,v_gvrb).
mapseag(vsup,v_mod).
mapseag(vsupir,v_mod).
mapseag(vt,v_gvrb).

eamapseag([o,d,n|_],vgc,v_avere).
eamapseag([o,d,n|_],vgc,v_essere).
eamapseag([e,r,e|_],vci,v_essere).
eamapseag([e,r,e|_],vci,v_avere).
eamapseag(W,vc,v_essere).
eamapseag(W,vcir,v_essere).
eamapseag(W,vc,v_avere).
eamapseag(W,vcir,v_avere).
eamapseag([o,d,n|_],auag,v_avere).
eamapseag([o,d,n|_],aueg,v_essere).
eamapseag([e,r,e|_],ausai,v_avere).
eamapseag([e,r,e|_],ausei,v_essere).
eamapseag([e,t,a|_],ausep,v_essere).
eamapseag([a,t,a|_],ausep,v_essere).
eamapseag([i,t,a|_],ausep,v_essere).
eamapseag([o,t,a|_],ausep,v_essere).
eamapseag(W,ausa,v_avere).
eamapseag(W,auair,v_avere).
eamapseag(W,ause,v_essere).
eamapseag(W,aueir,v_essere).

wmapseag(W,Tg,T):-
   mapseag(Tag,T),
   (Tag=v_pp, Tg=vppt
     ;
    (Tag=v_essere;Tag=v_avere), Tg=[aux, vc]
     ;
     Tag=v_clit, Tg=vcl
     ;
     Tag=v_mod, Tg=vsup
     ;
     check_vcat(Tag),Tg=v
     ;
     \+check_vcat(Tag),Tg=Tag),
   !.
wmapseag(W,Tg,T):-
  (T=v_avere;T=v_essere),
  stringof(List,W),
  reverse(List,Rev),
  eamapseag(Rev,Tg,T),
  !.

% v_mod, v_gvrb, v_pp, v_essere, v_avere, v_clit   
   
