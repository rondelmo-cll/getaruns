      disamb2([W1-Distr1-Mine0-1,W2-Distr2-Mine2-2|Disouts],Diss),
      !.      
   
disamb2([W-Distr-Mine-0,W1-Distr1-Mine1-3,W2-Distr2-Mine2-2|Disouts],[W-Distr-Mine-0|Diss]):-
      (atomic(Distr),A=Distr;Distr=[A]),
      A\=nil,
      on(B,Mine2),
      filtercats(A,W,B,W1,A1,B1),
       calltrim66(W,A1,Mine1,W2,Mine2,Mine0),
      disamb2([W1-Distr1-Mine0-1,W2-Distr2-Mine2-2|Disouts],Diss),
      !.      
   
/* 2-2-1*/
disamb2([W-Distr-Mine-2,W1-Distr1-Mine1-2,W2-Distr2-Mine2-N1|Disouts],Diss):-
      (calltrim22(Mine,Mine1,W,Mine2,Mine0)
       ;
      calltrim22(Mine,Mine1,W,Distr2,Mine0)),
      prunelist(Mine0,N,NewMine),
      disamb31([W-Distr-NewMine-N, W1-Distr1-Mine1-2,W2-Distr2-Mine2-N1|Disouts],Diss),
      !.      

/* call 40 - 2-1*/
disamb2([W-Distr-Mine-2,W1-Distr1-Mine1-1|Disouts],[W-Distr-n-0|Diss]):-
      Mine=[ag,n],
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      nouncats(B),
      wnit(W,n,Code),
      disamb2([W1-Distr1-Mine1-1|Disouts],Diss),
      !.            
disamb2([W-Distr-Mine-2,W1-Distr1-Mine1-1|Disouts],Diss):-
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      W\=perché,
      findall(Mine0,
                 (on(A,Distr),
                   filtercats(A,W,B,W1,A1,B1),
                     collecttrim2(Mine,B1,Mine0)), Trimms),
      appiattisci(Trimms,Tris),sort(Tris,Trims),
      evaluatetrims2(Trims,Mine0),
      Mine0\=[],
      disamb2([W-Distr-Mine0-1,W1-Distr1-Mine1-1|Disouts],Diss),
      !.      
disamb2([W-Distr-Mine-2,W1-Distr1-Mine1-1|Disouts],Diss):-
      (atomic(Distr1),B=Distr1;Distr1=[B]),
      findall(Mine0,
                 (on(A,Distr),
                   filtercats(A,W,B,W1,A1,B1),
                     collecttrim2(Mine,B1,Mine0)), Trimms),
      appiattisci(Trimms,Tris),sort(Tris,Trims),
      evaluatetrims2(Trims,Mine0),
      Mine0\=[],
      disamb2([W-Distr-Mine0-1,W1-Distr1-Mine1-1|Disouts],Diss),
      !.      

/* 2-0*/
