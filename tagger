:- style_check(-singleton).


simpletokenizer(InFile,OutFile):-
   repeat,
   /*seeing(DefaultInFile),
*/   see(InFile),
   read_atomics(OutFile),
   seen,
   !.

parse_file(X):-
   simpletokenizer(X, Phrase),
   tag_text(Phrase,Outs),
   mults(Outs,Out),
   write(Out).

tag_text([],[]):-!.
tag_text([W|Text],[W-Tag|Out]):-
    wl(W,Tag,F,Gen),
    tag_text(Text,Out),
     !.
tag_text([W|Text],[W-Tag|Out]):-
    s(W,Tag,F),
    tag_text(Text,Out),
     !.
tag_text([W|Text],[W-Tag|Out]):-
    select_punct(W, Tag),
    tag_text(Text,Out),
     !.
tag_text([W|Text],[W-[nw]|Out]):-
    tag_text(Text,Out),
     !.
 
 % wl(a_s_,[np],8,e).
 % s(abb, np, 1).

computefssbest(W,Tags):-
   \+fct(W),
   tolower(W,Low),\+fct(Low),
   allfs2(W,Cat,Allf),
   sort(Allf,Alls),
   (allfs1(W,Cat1,Allf1),
    sort(Allf1,Alls1),
    append(Alls,Alls1,Alls2)
    ;
    Alls2=Alls),
   getallfreqtags(Alls2,Tags),
   Tags\=[],
   !.

