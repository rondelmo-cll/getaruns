deletezeros([W-Distr-Mine-N,W1-Distr1-Mine1-N1,che-Distr2-Mine2-N2|Disouts],[W-Distr-clit-1,W1-Distr1-v-1,che-Distr2-Mine2-N2|Diss]):-
      member(Ca,Mine2),check_vcat(Ca),
      member(clit,Mine),
      member(Cat,Mine1),check_vcat(Cat),
      extract_subcat(W,Lem,SubCat),
      allproptrs(Props),
      member(Tr,SubCat),
      member(Tr,Props),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-Mine-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2|Disouts],[W-Distr-clit-1,W1-Distr1-clit-1|Diss]):-
      member(Ca,Mine2),check_vcat(Ca),
      (W\='Se',W\=se;W\=si),
      member(clit,Mine),
      member(clit,Mine1),
      deletezeros([W2-Distr2-v-1|Disouts],Diss),
      !.      
deletezeros([W-Distr-Mine-N,W1-Distr1-Mine1-N1|Disouts],[W-Distr-clit-1,W1-Distr1-v-1|Diss]):-
      member(Ca,Mine1),check_vcat(Ca),
      member(clit,Mine),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-Mine1-N,W1-Distr1-Mine-N1|Disouts],[W-Distr-v-1,W1-Distr1-clit-1|Diss]):-
      member(Ca,Mine1),check_vcat(Ca),
      member(clit,Mine),
      stringof(List,W),
      reverse(List,[r|Rev]),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([fa-Distr-Mine-N,W1-Distr1-Mine1-N1|Disouts],[fa-Distr-v-1,W1-Distr1-v-1|Diss]):-
      (member(v,Mine1),\+member(n,Mine1);Mine1=v),
      member(v,Mine),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([fa-Distr-Mine-N,W1-Distr1-Mine1-N1|Disouts],[fa-Distr-v-1,W1-Distr1-n-1|Diss]):-
      (member(n,Mine1);Mine1=n),
      member(v,Mine),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-Mine-N,W1-Distr1-Mine1-N1|Disouts],[W-Distr-ppas-1|Diss]):-
      (is_list(Mine1),member(p,Mine1);atomic(Mine1),Mine1=p),
      member(ppas,Mine),
      deletezeros([W1-Distr1-p-1|Disouts],Diss),
      !.      
/*
deletezeros([W-Distr-Mine-N,W1-Distr1-Mine1-N1|Disouts],[W-Distr-vppt-1,W1-Distr1-p-1|Diss]):-
      (is_list(Mine1),member(p,Mine1);atomic(Mine1),Mine1=p),
      member(ag,Mine),\+member(n,Mine),
      (member(v,Mine);member(vppt,Mine)),
      deletezeros(Disouts,Diss),
      !.      
*/
deletezeros([W-Distr-Mine-2,W1-Distr1-Mine1-N1|Disouts],[W-Distr-neg-1,W1-Distr1-v-1|Diss]):-
      member(Ca,Mine1),check_vcat(Ca),
      member(neg,Mine),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-Mine-1,W1-Distr1-Mine1-2|Disouts],[W-Distr-Mine-1,W1-Distr1-ag-1|Diss]):-
      (member(avv,Mine);member(ppas,Mine)),
      member(Ca,Mine1),check_vcat(Ca),
      (member(ppas,Mine1);member(vppt,Mine1)),
      member(ag,Mine1),
      Disouts=[W2-Distr2-Mine2-N2|_],
      \+prepos(W2),\+prepart(_,W2,_),
      nogen_member(Cat,Mine2),\+preposcats(W2,Cat),
      deletezeros(Disouts,Diss),
      !.      
/*deletezeros([W-Distr-Mine-1,W1-Distr1-Mine1-2|Disouts],[W-Distr-art-1,W1-Distr1-n-1|Diss]):-
      member(art,Mine),member(n,Mine1),
      deletezeros(Disouts,Diss),
      !.      
*/
deletezeros([W-Distr-Mine-1,W1-Distr1-Mine1-2|Disouts],[W-Distr-Mine-1,W1-Distr1-vppt-1|Diss]):-
      member(Ca,Mine1),check_vcat(Ca),
      member(ppas,Mine1),\+member(n,Mine1),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-Mine-0,W1-Distr1-Mine1-2|Disouts],[W-Distr-v-0,W1-Distr1-art-1|Diss]):-
      member(Ca,Mine),check_vcat(Ca),
      (is_list(Mine1),(Mine1=[art];member(art,Mine1));
       atomic(Mine1), Mine1=art),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-p-0,W1-Distr1-Mine1-2|Disouts],[W-Distr-p-0,W1-Distr1-n-1|Diss]):-
      member(Ca,Mine1),check_vcat(Ca),
      (member(ppas,Mine1);member(vppt,Mine1)),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-part-N,W1-Distr1-Mine1-N1|Disouts],[W-Distr-part-1,W1-Distr1-Ca-1|Diss]):-
      member(Ca,Mine1),nouncats(Ca),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-Mine-N,W1-Distr1-Mine1-N1|Disouts],[W-Distr-art-1,W1-Distr1-Cat-1|Diss]):-
      (is_list(Mine),(Mine=[art];member(art,Mine));
       atomic(Mine), Mine=art),
       member(Ca,Mine1),
       (nouncats(Ca),Ca\=mw,Cat=Ca
       ;
       member(ag,Mine1),predag(W1),
       psl(W1,s,_,L,_,Fre),5<Fre,
       \+member(avv,Mine1),Cat=n),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-Mine-N,W1-Distr1-Mine1-N1|Disouts],[W-Distr-Mine-1,W1-Distr1-Cat-1|Diss]):-
      (is_list(Mine),(Mine=[part];Mine=[p];member(part,Mine);member(p,Mine));
       atomic(Mine), (Mine=p;Mine=part)),
       member(Ca,Mine1),
       (nouncats(Ca),Ca\=mw,Cat=Ca
       ;
       member(ag,Mine1),predag(W1),
       psl(W1,s,_,L,_,Fre),5<Fre,
       \+member(avv,Mine1),Cat=n),
       Disouts=[W2-_-C-_|_],
       (atomic(C),\+nouncats(C);is_list(C),member(Ca2,C),\+nouncats(Ca2)),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-Mine-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2|Disouts],[W-Distr-Mine-1,W1-Distr1-ag-1,W2-Distr2-n-N2|Diss]):-
      (is_list(Mine),(Mine=[art];member(art,Mine));
       atomic(Mine), Mine=art;
     (W=una;W=uno;W=un),
      (member(num,Mine);Mine=num)
       ),
       member(Ca,Mine1),
       member(ag,Mine1),
       member(Ca1,Mine2),
       (nouncats(Ca1),Cat1=Ca1
       ;
       member(ag,Mine2),
       psl(W2,s,_,L,_,Fre),5<Fre,
       \+member(avv,Mine2),Cat1=n),
       Disouts=[W2-_-C-_|_],
       (atomic(C),\+nouncats(C);is_list(C),member(Ca2,C),\+nouncats(Ca2)),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([Uno-Distr-Mine-N,W1-Distr1-Mine1-N1|Disouts],[Uno-Distr-num-1,W1-Distr1-q-1|Diss]):-
     (Uno=una;Uno=uno),
      (member(num,Mine1);Mine1=num;
       selectdi(W1)),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([Uno-Distr-Mine-N,W1-Distr1-Mine1-N1|Disouts],[Uno-Distr-art-1,W1-Distr1-Mine1-1|Diss]):-
     (Uno=una;Uno=uno),
      (\+member(num,Mine1);Mine1\=num),
      deletezeros(Disouts,Diss),
      !.      
deletezeros([W-Distr-Mine-0|Disouts],[W-Distr1-Mine-1|Diss]):-
      delzero(Distr,Mine,Distr1),
      deletezeros(Disouts,Diss),
      !.
deletezeros([W-Distr-Mine-N|Disouts],[W-Distr-neg-N|Diss]):-
      negs(W,_,_),member(neg,Mine),\+member(q,Mine),
      deletezeros(Disouts,Diss),
      !.
deletezeros([W-Distr-Mine-N|Disouts],[W-Distr-Mine-N|Diss]):-
      deletezeros(Disouts,Diss),
      !.


predag(W):-
    stringof(List,W),
    (de(W,Cats)
      ;
      reverse(List,[_|Rev]),
      reverse([o|Rev],Le),
      stringof(Le,Lem),
      de(Lem,Cats)),
      member(a:F,Cats),
      (F=t;F=n;F=prov;F=p1;F=p2;F=pev;F=ev;F=p3;F=arg;
      F=t:_;F=n;F=prov:_;F=p1:_;F=p2:_;F=pev:_;F=ev:_;F=p3:_;F=arg:_),
    !.
% a:z a:inv a:c a:sup:ev a:f a:poss a:avv
% a:attr

predets(ambedue).
predets(entrambi).
predets(entrambe).
predets(tutto).
predets(tutti).
predets(tutta).
predets(tutte).

checkparts(W):-      
      wl(W,Cats,Fr,Ge), 
      (on(part,Cats);
      on(partd,Cats);
      on(partda,Cats)), 
      !.

delzero([Tag|Distr],Mine,[Tag]):-
      (atomic(Mine),A=Mine;Mine=[A]),
      matchdistrs(Tag,A),
      !.
delzero([Tag|Distr],Mine,Distr1):-
      (atomic(Mine),A=Mine;Mine=[A]),
      \+ matchdistrs(Tag,A),
      delzero(Distr,Mine,Distr1),
      !.

rightcont([W-Distr-Mine-N|Right]):-
      \+ nominalcat(Mine),
      !.
nominalcat(Mine):-
      (atomic(Mine),A=Mine;Mine=[A];Mine\=[A],on(A,Mine)),
      (A=n;A=np;A=npro;A=nh;A=nt;A=num;A=nf;A=agn;A=ag;A=mw),
      !.
checkppre(W,Out):-
      member(W-Feats,Out),
      member(cat=Cat,Feats),
      Cat=ppre,
      !.
      
allcatsde(W,Cas):-
      de(W,Cats),
      findall(Ca,(member(C,Cats),
                term_to_atom(C,Catt),
                concat_atom(X,:,Catt),
                X=[Ca,S|_]),Catts),
       sort(Catts,Cass),
       checkcatsde(Cass,Cas),
       !.

checkcatsde([],[]):-!.
checkcatsde([a|Cass],[ag|Cas]):-
       checkcatsde(Cass,Cas),
       !.
checkcatsde([N|Cass],[N|Cas]):-
       checkcatsde(Cass,Cas),
       !.

detectagn(W,n,0,Right):-
      de(W,Cats),
      on(Cat,Cats),
      term_to_atom(Cat,Catt),
      concat_atom(X,:,Catt),
      X=[Ca,S|_],
       Ca=a, (S=prov;S=p1;S=p2;S=pev;S=p3),
       rightcont(Right),
     !.
detectagn(W,n,0,Right):-
    stringof(List,W),
    reverse(List,[V|Rev]),
    (V=e,V1=a;V=e,V1=i;V=a,V1=e;V=i,V1=e;V=i,V1=o;V=a,V1=o;V=o,V1=a;V=o,V1=i),
    reverse([V1|Rev],Lis),
    stringof(Lis,W1),
      de(W1,Cats),
      on(Cat,Cats),
      term_to_atom(Cat,Catt),
      concat_atom(X,:,Catt),
      X=[Ca,S|_],
      Ca=a, (S=prov;S=p1;S=p2;S=p3),
       rightcont(Right),
      !.

disambdismw(W1,W2-Distr1-Mine1-N1):-
     ids(mw-W1,_-W2)
     ;
     eds(n-W1,_-W2)
     ;
     toupper(W1,Ww1),
     toupper(W2,Ww2),
     ids(mw-Ww1,_-Ww2)
     ;
     toupper(W1,Ww1),
     toupper(W2,Ww2),
     eds(n-Ww1,_-Ww2),
     !.
disambdi(W-Distr-Mine-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,pt-1):-
     (atomic(Mine2), v=Mine2;
        on(v,Mine2), 
        (\+ on(n,Distr2);
          on(n,Distr2), on(v,Distr));
       on(Cat,Mine2),check_all_verbs(Cat),
               (\+ on(n,Distr2);
          on(n,Distr2), on(v,Distr))
          ),
     extract_subcat(W2,Lem,SubCat), 
     (W2=Lem;
      get_root(W2,C2,R2-S2), 
      get_root(Lem,C22,R1-SubC),
      R2=R1),
     !.
disambdi(W-Distr-Mine-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,Mine0-N1):-
     subtract(Mine1,[pt],Mine0),
     !.

disambche(W-Distr-punto-N,che-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,relin-1):-
     !.
disambche(W-Distr-[dirs]-N,che-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,q-1):-
     W2\=cosa,
     !.

disambche(W-Distr-Mine-N,che-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,pk-1):-
     (atomic(Mine), v=Mine;on(v,Mine);on(Cat,Mine),check_all_verbs(Cat)),
     check_subcat_verb(W,Lem),
     !.
disambche(W-Distr-Mine-N,che-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,q-1):-
     (atomic(Mine), p=Mine;on(p,Mine)),
     (atomic(Mine2), n=Mine2;on(n,Mine2)),
     !.
disambche(W-Distr-Mine-N,che-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,Mine1-N1):-
     (atomic(Mine), C1=Mine;on(C1,Mine)),
     (atomic(Mine2), C2=Mine2;on(C2,Mine2)),
     chet(C1-cong-C2-_),
     \+ check_subcat_verb(W,Lem),
     !.
disambche(W-Distr-Mine-N,che-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,Mine0-N1):-
     subtract(Mine1,[cong],Min),
     subtract(Min,[pk],Mine0),
     !.
disambche(W-Distr-Mine-N,W1-Distr1-Mine1-N1,che-Distr2-Mine2-N2,pk-1):-
     (atomic(Mine), v=Mine;on(v,Mine);on(Cat,Mine),check_all_verbs(Cat)),
     check_subcat_verb(W,Lem),
     (Lem=essere,
     (atomic(Mine1), (Mine1=a;ag=Mine1);
            (on(ag,Mine1);on(a,Mine1))
            )
     ;
     Lem\=essere), Mine1\=[dirs],
     !.
disambche(W-Distr-Mine-N,W1-Distr1-Mine1-N1,che-Distr2-Mine2-N2,pk-1):-
     (atomic(Mine), v=Mine;on(v,Mine);on(Cat,Mine),check_all_verbs(Cat)),
     checkroots(W,Root-Cats),
     (Root=res;Root=rend;Root=ten;Root=tenut;Root=f),
      W1=conto,
     !.
disambche(W-Distr-Mine-N,W1-Distr1-Mine1-N1,che-Distr2-Mine2-N2,pk-1):-
%     (atomic(Mine), art=Mine;on(art,Mine)),
     nfs(W1),
     !.
disambche(W-Distr-Mine-N,W1-Distr1-Mine1-N1,che-Distr2-Mine2-N2,rel-1):-
     (atomic(Mine), 
           (Mine=ag;n=Mine);
      on(n,Mine);on(ag,Mine))
       ;
     (atomic(Mine1), 
           (Mine=ag;n=Mine1);
      on(n,Mine1);on(ag,Mine1)),
     !.
 
disambsubargs1(W-Distr-punto-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,D,Mine0-1):-
     (\+ on(clit,Mine2);
      on(clit,Mine2), D=[W3-Distr3-Mine3-N3|_],\+ on(n,Mine3)),
     subtract(Mine1,[clit],Mine0),
     !.
/*
disambsubargs(W-Distr-Mine-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,D,pk-1):-
     (atomic(Mine), v=Mine;on(v,Mine);on(Cat,Mine),check_all_verbs(Cat)),
     check_subcat_verb(W,Lem), member(C,Mine1),
     ;
     (atomic(Mine1), v=Mine1;on(v,Mine1);on(Cat,Mine1),check_all_verbs(Cat)),
     check_subcat_verb(W1,Lem1),
     !.
*/
disambsubargs(W-Distr-Mine-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,D,clit-1):-
      W2=se,D=[W3-Distr3-Mine3-N3|_],on(clit,Mine3),W3\=si,
      !.
disambsubargs(W-Distr-Mine-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,D,Mine0-N2):-
     subtract(Mine2,[clit],Mine0),
     !.
disambsubargs(W-Distr-Mine-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,D,Mine2-N2):-
     !.
 
disambsubargsp(W-Distr-Mine-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,pt-1):-
     check_subcat_verbp(W,Lem)
     ;
     check_subcat_verbp(W1,Lem1),
     !.
disambsubargsp(W-Distr-Mine-N,W1-Distr1-Mine1-N1,W2-Distr2-Mine2-N2,Mine0-N2):-
     subtract(Mine2,[pt],Mine0),
     !.

diss([],[]):-!.
diss([W-[punt]|Outs],[W-[punt]-[punt]-1|Diss]):-
      diss(Outs,Diss),
      !.
diss([W-[punto]|Outs],[W-[punt]-[punto]-1|Diss]):-
      diss(Outs,Diss),
      !.
diss([W-[puntint]|Outs],[W-[punt]-[puntint]-1|Diss]):-
      diss(Outs,Diss),
      !.
diss([W-[par]|Outs],[W-[punt]-[par]-1|Diss]):-
      diss(Outs,Diss),
      !.
diss([W-[star]|Outs],[W-[punt]-[star]-1|Diss]):-
      diss(Outs,Diss),
      !.
diss([W-[dots]|Outs],[W-[punt]-[dots]-1|Diss]):-
      diss(Outs,Diss),
      !.
diss([W-[num]|Outs],[W-[arg_det,adj,entities]-num-0|Diss]):-
      diss(Outs,Diss),
      !.
diss([W-[dirs]|Outs],[W-[punt]-[dirs]-1|Diss]):-
      diss(Outs,Diss),
      !.
diss([W-[nw]|Outs],[W-[a,n]-[nw]-1|Diss]):-
      diss(Outs,Diss),
      !.
diss([W-T|Outs],[W-T1-T-0|Diss]):-
      (atomic(T);T=[A]),
      ambdis(W,T1),     
      diss(Outs,Diss),
      !.
diss([[W-T]|Outs],[W-T1-T-0|Diss]):-
      (atomic(T);T=[A]),
      ambdis(W,T1),     
      diss(Outs,Diss),
      !.
diss([W-T|Outs],[W-T1-T-0|Diss]):-
      (atomic(T);T=[A]),
      ambdisupun(W,T1),     
      diss(Outs,Diss),
      !.
diss([[W-T]|Outs],[W-T1-T-0|Diss]):-
      (atomic(T);T=[A]),
      ambdisupun(W,T1),     
      diss(Outs,Diss),
      !.
diss([W-T|Outs],[W-T1-T-2|Diss]):-
      ambdis(W,T1),     
      diss(Outs,Diss),
      !.
diss([W-T|Outs],[W-T1-T-2|Diss]):-
      ambdisupun(W,T1),     
      diss(Outs,Diss),
      !.
/*
diss([W-T|Outs],[W-T1-T-1|Diss]):-
      uniqdis(W,T1),     
      diss(Outs,Diss),
      !.
*/
diss([W-T|Outs],[W-T1-T-1|Diss]):-
      uniqdisup(W,T1),     
      diss(Outs,Diss),
      !.
diss([W-T|Outs],[W-[]-[relin]-1|Diss]):-
      firstword(W),
      W='Chi',
      diss(Outs,Diss),
      !.
diss([W-T|Outs],Diss):-
      atomic(W),
      tolower(W,W1),
      diss1([W1-T|Outs],Diss),
      !.
diss([[W-T]|Outs],Diss):-
      atomic(W),
      tolower(W,W1),
      diss1([W1-T|Outs],Diss),
      !.
diss([W-T|Outs],[W-[nil]-T-L|Diss]):-
      (atomic(T),L=0;T=[A],L=0;T\=[A],L=2),
      diss(Outs,Diss),
      !.
diss([[W-T]|Outs],[W-[nil]-T-L|Diss]):-
      (atomic(T),L=0;T=[A],L=0;T\=[A],L=2),
      diss(Outs,Diss),
      !.

/*
diss1([W-T|Outs],[W-T1-T-1|Diss]):-
      uniqdis(W,T1),     
      diss(Outs,Diss),
      !.
*/
diss1([W-T|Outs],[W-T1-T-1|Diss]):-
      uniqdisup(W,T1),     
      diss(Outs,Diss),
      !.
diss1([W-T|Outs],[W-T1-T-0|Diss]):-
      (atomic(T);T=[A]),
      ambdis(W,T1),     
      diss(Outs,Diss),
      !.
diss1([W-T|Outs],[W-T1-T-0|Diss]):-
      (atomic(T);T=[A]),
      ambdisupun(W,T1),     
      diss(Outs,Diss),
      !.
diss1([W-T|Outs],[W-T1-T-2|Diss]):-
      ambdis(W,T1),     
      diss(Outs,Diss),
      !.
diss1([W-T|Outs],[W-T1-T-2|Diss]):-
      ambdisupun(W,T1),     
      diss(Outs,Diss),
      !.

dc('a:p1', 1, adj).
dc('a:p3', 1, adj).
dc('a:pev', 1, adj).
dc('a:p4', 1, adj).
dc('a:p2', 1, adj).
dc('a:prov', 1, adj).
