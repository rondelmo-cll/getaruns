decodecatsst(0,noun,location,np).
decodecatsst(1,noun,group,nh).
decodecatsst(1,noun,person,nh).
decodecatsst(P,noun,Cat,n).
decodecatsst(P,adj,Cat,ag).
decodecatsst(P,verb,Cat,v).


guessallcat([],[]):-!.
guessallcat([Pred-Feats|Out],[Cat|Cats]):-
    guesscat(Feats,Cat),
    guessallcat(Out,Cats),
    !.
guessallcat(Out,Cats):-
    guessallcat(Out,Cats),
    !.
    
check_nouncats(Ca,Cat):-
     nouncats(Ca),
     (Catt=npl,Cat=n
      ;
      Catt\=npl,Cat=Catt),
      !.
checknominals(W,Pol,Low,Tagg):-
    Pol=0,
    stringof(List,W),
    reverse(List,[V|Rev]),
    (V=e,V1=a;V=e,V1=i;V=a,V1=e;V=i,V1=e;V=i,V1=o;V=o,V1=i),
    reverse([V1|Rev],Lis),
    stringof(Lis,W1),
    lm(W1,Syl,Tags),
    transllmtags(Tags,Tag),
    append([v],[Tag],Tagg),
     !.

transllmtags(Tags,Tag):-
     atomic(Tags),
     talm(Tags,Tag),
     !.
transllmtags(Tags,Tag):-
     is_list(Tags),
     talms(Tags,Tag),
     !.

talms([],[]):-!.
talms([Tg|Tags],[T|Tag]):-
   talm(Tg,T),
   talms(Tags,Tag),
   !.
talms([Tg|Tags],Tag):-
   talms(Tags,Tag),
   !.

