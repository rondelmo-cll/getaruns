ttgtext(N,W,Pol,Low,W-Cat):-
   recover_descs(W,List),
   is_list(List),
   (reverse(List,[Num|_]),
    integer(Num),
    List=[A|_],\+integer(A),
    Cat=np
    ;
    reverse(List,[Num|_]),
    integer(Num),
    Cat=num
    ;
    member(First,List),
    getlem(First,Ca,Lem-Feat),
    check_nouncats(Ca,Cat)
    ),
    !.
ttgtext(N,W,1,Low,W-[nh]):-
    N\=1,!.
ttgtext(N,W,1,Low,Low-Tag):-
    N=1,
    W\=Low,
   tagtext1(Low,Low-Tag),
   !.
ttgtext(N,W,Pol,Low,W1-Tag):-
    stringof(Lis,W),
    reverse(Lis,Rev),
    Rev=[è|Rest],
    Rev1=[é|Rest],
    reverse([é|Rest],List),
    stringof(List,W1),
    tagtext1(W1,W1-Tag),
     !.    
