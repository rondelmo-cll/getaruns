    !.

allcatsili(W,Tag):-
    (its(W,Cats,Tags);mist(W,_,Cats,Tags);mist(_,W,Cats,Tags)),
    findall(T1,(member(T:N,Tags),(T=p,T1=[nh,np];T\=p,T1=T)),Tagss),
    appiattisci(Tagss,Tagg),
    sort(Tagg,Tag),
    !.
allcatsili(W,[avv]):-
    rist(W,K),
    !.
exploresemantic(Head,Cat):-
                  wnit(Head,C,Code),makecat(C,Cat)
                  ;
                  wndang(_,Head),Cat=[n]
                  ;
                  wnmed(_,Head),Cat=[n]
                  ;
                  wnrol(_,Head),Cat=[n,nh]
                  ;
                  wnwar(_,Head),Cat=[n]
                  ;
                  wnist(_,Head),Cat=[n]
                  ;
                  ecwn(Head,_),Cat=[n]
                  ;
                  dmc(Head),Cat=[n]
                  ;
                  swn(Head,Ca,Val),makecat(Ca,Cat)
                  ;
                  nclp(Head,Pos,Cas),makecats(Cas,Cat)
    ;  
    recover_descs(Head,Lis),islist(Lis),
    jrc(Lis,org,_), Cat=[np]
    ;
    if_first_name(Head),\+gi(Head,Loc,Clas,Se),
    Cat=[nh]
    ;
    mwec(Ws, Head), Cat=[n]
    ;
    gi(Head,Loc,Clas,Se),Cat=[n,np]
    ;
    alterato(Head,Hea,Alt,Gen,Num),
    wnit(Hea,C,Code),Cat=[n],
    !.

makecats([First|Cas],[ag]):-
    (First=pred;First=attr;First=restr),
    !.
makecats([First|Cas],[v]):-
    verbcatsyn(First),
    !.
makecats(Cas,[n]):-
    !.

verbcatsyn(tr).
verbcatsyn(refl).
verbcatsyn(refl_in).
verbcatsyn(psych2).
verbcatsyn(psych1).
verbcatsyn(unac).
verbcatsyn(unerg).
verbcatsyn(cop).
verbcatsyn(raisn).

makecat(r,avv).
makecat(a,ag).
makecat(n,n).
makecat(v,v).

guesscat(Feats,Cat):-
    member(cat=Cats,Feats),
    (Cats=intj,Cat=inter
     ;
     Cats=adj,Cat=ag
     ;
     Cats=ppas,Cat=ag
     ;
     Cats=noun,Cat=n
     ;
     Cats=num,Cat=num
     ;
     Cats=adv,Cat=av
     ;
     Cats=verb,
     member(mood=Mood,Feats),
      (Mood=ind,Cat=v
       ;
       Mood=inf,Cat=vit
       ;
       Mood=ger,Cat=vgt
       ;
       Mood=part,Cat=vppt
       ; Cat=virt)
       ),
       !.
guesscat(Feats,Cat):-
    member(cat=Cat,Feats),
    !.
%guesscat(Feats,v):-!.

tagtext1(W,W-Tag):-
     lm(W,Syl,Tags),
     transllmtags(Tags,Tag),
     !.
tagtext1(W,W-Tag):-
    s(W,Tag,F),
     !.
tagtext1(W,W-Tags):-
    l(W,Tag),
    extractcat(Tag,Tags),
    !.
tagtext1(W,W-Tags):-
    t(W,Tag),
    extractcat(Tag,Tags),
     !.
tagtext1(W,W-Tags):-
    callde(W,Tag),
    extractcat(Tag,Tags),
     !.
tagtext1(W,W-Tags):-
    tml(W,Tag),
    extractcat(Tag,Tags),
     !.
tagtext1(W,W-Tag):-
    wl(W,Tag,F,Gen),
     !.
/*
tagtext1(W,W-[n]):-
    checkt(W)
    ;
    chabbr(W)
    ;
    charl(W)
    ;
    checkab(W),
     !.
*/
extractmobycats(Cats,MCats):-
    stringof(List,Cats),
    length(List,L),
    choosecats(L,List,MCats),
    !.
extractdccats(Cats,MCats):-
    length(Cats,L),
    choosedccats(L,Cats,MCats),
    !.
choosedccats(1,List,[Cat]):-
    List=[C],
    dcconv(C,Cat),
    !.
choosedccats(L,List,Cats):-
    1 < L,
    dcconvs(List,Cats),
    !.
dcconvs([],[]):-!.
dcconvs([C|List],[Cat|Cats]):-
    dcconv(C,Cat),
    dcconvs(List,Cats),
    !.
dcconvs([C|List],[v|Cats]):-
    concat_atom(Lis,:,C),
    on(v,Lis),
    dcconvs(List,Cats),
    !.
dcconv(n,n).
dcconv(npl,n).
dcconv(art,art).
dcconv(adv,avv).
dcconv(adj,agg).
dcconv(prep,prep).
dcconv(conj,cong).
dcconv(pron,pron).
dcconv(relpron,rel).
dcconv(posspron,poss).
dcconv(int,int).

choosecats(1,List,[Cat]):-
    List=[C],
    mbwn(C,Cat),
    !.
choosecats(L,List,Cats):-
    1 < L,
    mbwns(List,Cats),
    !.
mbwns([],[]):-!.
mbwns([C|List],[Cat|Cats]):-
    mbwn(C,Cat),
    mbwns(List,Cats),
    !.

mbwn('N',n).
mbwn(p,n).
mbwn('P',prep).
mbwn('A',agg).
mbwn(v,avv).
mbwn(r,ind).
mbwn('V',v).
mbwn(t,vt).
mbwn(i,vi).
mbwn('C',cong).
mbwn('D',q).
mbwn(h,n).
mbwn(j,inter).

/*
mobywn
mbw(Word,Cat),
N	nomi
p	nomi plurali
P	preposizioni
A	aggettivi
v	avverbi
r	determinanti, pronomi
t	verbi transitivi
i	verbi intransitivi
V	verbi (ergativi?)
C	congiunzioni
D	determinatori
h	nomi hyphenati
j	interiezione
*/

checklocsall(L,W,[n]):-
     eloc1(W,L)
     ;
     eloc2(W,L)
     ;
     eloc3(W,L)
     ;
     loc1(W,L)
     ;
     loc2(W,L)
     ;
     loc3(W,L),
     !.

callde(W,Tag):-
   findall(Cat,(
              den(W),Cat=n;
              dev(W),Cat=v;
              dea(W),Cat=ag),Tag),
              Tag\=[],!.
              
checkroots(W,Tag):-
    stringof(List,W),
    length(List,L),
    search_suffix(L,List,Tag),
     !.

guesser(List):-
    reverse(List,[A,B,C,D,E,F|Rest]),
    stringof([F,E,D,C,B,A],Suf),
    sf(Suf,Tags),
    findall(Sf, (on(Su,Tags),
        atom_to_term(Su,Sf,L)),Sfs),
    (on(suff:a:a,Sfs);
     on(suff:n:a,Sfs);
     on(suff:n:n:_,Sfs)),
    !.
guesser(List):-
    reverse(List,[A,B,C,D,E|Rest]),
    stringof([E,D,C,B,A],Suf),
    sf(Suf,Tags),
    findall(Sf, (on(Su,Tags),
        atom_to_term(Su,Sf,L)),Sfs),
    (on(suff:a:a,Sfs);
     on(suff:n:a,Sfs);
     on(suff:n:n:_,Sfs)),
    !.
guesser(List):-
    reverse(List,[A,B,C,D|Rest]),
    stringof([D,C,B,A],Suf),
    sf(Suf,Tags),
    findall(Sf, (on(Su,Tags),
        atom_to_term(Su,Sf,L)),Sfs),
    (on(suff:a:a,Sfs);
     on(suff:n:a,Sfs);
     on(suff:n:n:_,Sfs)),
    !.
guesser(List):-
    reverse(List,[A,B,C,D|Rest]),
    stringof([C,B,A],Suf),
    sf(Suf,Tags),
    findall(Sf, (on(Su,Tags),
        atom_to_term(Su,Sf,L)),Sfs),
    (on(suff:a:a,Sfs);
     on(suff:n:a,Sfs);
     on(suff:n:n:_,Sfs)),
    !.
extractcat(In,Out):-
          findall(T,(
                on(T1,In), getcatop(T1,T)),
                Ts), appiattisci(Ts,Tss),sort(Tss,Out),
                Out\=[],
     !.
extractcat(In,In):-!.

getcatop(A,Cat):-
    atomic(A),
    concat_atom(X,:,A),
    (X=[Ca,Der,S|_], \+ integer(S);
    X=[Ca,S|_]),
    (Ca=suff, Cat=S;Ca\=suff, translatecats(Ca,S,Cat)),
    !.
getcatop(A,Cat):-
    \+ atomic(A),
    term_to_atom(A,A1),
    concat_atom(X,:,A1),
    (X=[Ca,Der,S|_], \+ integer(S);
    X=[Ca,S|_]),
    (Ca=suff, Cat=S;Ca\=suff, translatecats(Ca,S,Cat)),
    !.
getcatop(A,A):-
    atomic(A),
    !.
disambs(Outs,Dissam):-
      diss(Outs,Disouts),
      deletezeross(0,Disouts,Diss),
      deletezz(Diss,Diss0),
      disamb0(0,Diss0,Diss1),
      tevaluate(Diss1,[Recall,Count0,Count1],Dissam),
      !.
      
check_subcat_verb(Verb,Lem):-
      extract_subcat(Verb,Lem,SubCat),
      checksubcat(SubCat),
      !.
check_subcat_verb(Verb,Lem):-
      tolower(Verb,Low),Low\=Verb,
      extract_subcat(Low,Lem,SubCat),
      checksubcat(SubCat),
      !.
      
check_subcat_verbp(Verb,Lem):-
      extract_subcat(Verb,Lem,SubCat),
      checksubcatp(SubCat),
      !.
  
% v(serv,	v,	3,	inac_prop-[a,	 per]
%checksubcatp([]):-!.
checksubcatp([_-Sub|SubCat]):-
       on(per,Sub),
       !.
checksubcatp([Sub|SubCat]):-
     stringof(List,Sub),
     reverse(List,[r,e,p,_|RevList]),
      !.
checksubcatp([Sub|SubCat]):-
      checksubcatp(SubCat),
      !.


%checksubcat([]):-!.
checksubcat([Sub|SubCat]):-
     stringof(List,Sub),
     reverse(List,[p,o,r,p|RevList]),
      !.
checksubcat([Sub|SubCat]):-
     stringof(List,Sub),
     reverse(List,[p,o,p,o,r,p|RevList]),
     !.
checksubcat([Sub|SubCat]):-
     stringof(List,Sub),
     reverse(List,[x,p,o,r,p|RevList]),
     !.
checksubcat([Sub|SubCat]):-
     stringof(List,Sub),
     reverse(List,[t,n,i,p,o,r,p|RevList]),
     !.
checksubcat([Sub|SubCat]):-
      checksubcat(SubCat),
      !.

selectone(W-[nil]-Mine-N,W-Cat-1,1):-
      (atomic(Mine),A=Mine;Mine=[A]),
      matchdistrs(Cat,A),
      !.
selectone(W-Distr-Mine-0,W-Cat-1,1):-
      (atomic(Distr),A=Distr;Distr=[A]),
      A=Cat,
      !.
selectone(W-Distr-Mine-0,W-Cat-1,1):-
      (atomic(Mine),A=Mine;Mine=[A]),
      matchdistrs(Cat,A),
      !.
selectone(W-Distr-Mine-1,W-Cat-1,1):-
      (atomic(Mine),A=Mine;Mine=[A]),
      matchdistrs(Cat,A),
      !.
selectone(W-Distr-Mine-1,W-Cat-1,1):-
      (atomic(Distr),A=Distr;Distr=[A]),
      A=Cat,
      !.
selectone(W-Distr-Mine-2,W-Cat-1,1):-
      on(Cat,Distr),
      on(A,Mine),
      matchdistrs(Cat,A),
      !.
selectone(W-[nil]-Mine-2,W-Cat-1,1):-
      on(A,Mine),
      matchdistrs(Cat,A),
      !.
selectone(W-Distr-Mine-3,W-Cat-1,1):-
      on(Cat,Distr),
%      on(Prob-A-_-_-_,Mine),
      on(Prob-_-_-_-A,Mine),
      matchdistrs(Cat,A),
      !.
selectone(W-Distr-Mine-N,W-Cat-1,1):-
      atomic(Mine),
      matchdistrs(Cat,Mine),
      !.
selectone(W-Distr-Mine-N,W-Distr-1,1):-
      atomic(Distr),
      !.
selectone(W-Distr-Mine-N,W-Cat-N,0):-
      !.

tevaluate(Disoutss,[Recall,Count0,Count1],Diss):-
      tevalalls(Disoutss,Diss,Ev),
      findall(0,on(0,Ev),Zeros),
      findall(1,on(1,Ev),Ones),
      length(Diss,Cats),
      length(Zeros,Count0),
      length(Ones,Count1),
      Recall is Count1 / Cats * 100,
      !.


tevalalls([],[],[]):-!.

/*
tevalalls([W-Distr-Mine-N,W1-Distr1-Mine1-3,W2-Distr2-Mine2-N2|Disoutss],[W-Cat-N0|Diss],[N0|Ev]):-
      selectone(W-Distr-Mine-N,W-Cat-N0,N0),
      is_list(Distr1),
      is_list(Mine1),
      length(Distr1,L1),
      length(Mine1,L2), L2=<L1,
      sort(Mine1,Frs),reverse(Frs,[_-_-_-Cat2|Rev]),
      on(Cat1,Distr1),
      matchdistrs(Cat1,Cat2),
      tevalalls([W1-Distr1-Cat1-1,W2-Distr2-Mine2-N2|Disoutss],Diss,Ev),
      !.
*/
tevalalls([W-[nil]-Mine-0|Disoutss],[W-Mine-1|Diss],[1|Ev]):-
      (atomic(Mine),A=Mine;Mine=[A]),
      matchdistrs(Cat1,A),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-[nil]-Mine-1|Disoutss],[W-Mine-1|Diss],[1|Ev]):-
      (atomic(Mine),Cat=Mine;Mine=[Cat];is_list(Mine), on(Cat,Mine)),
      matchdistrs(Cat1,Cat),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-[nil]-Mine-2|Disoutss],[W-Mine-1|Diss],[1|Ev]):-
      on(A,Mine),
      matchdistrs(Cat1,A),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-N,W1-Distr1-Mine1-2,W2-Distr2-Mine2-N2|Disoutss],[W-Cat-N0|Diss],[N0|Ev]):-
      selectone(W-Distr-Mine-N,W-Cat-N0,N0),
      is_list(Distr1),
      is_list(Mine1),
      length(Distr1,L1),
      length(Mine1,L2), L1=<L2,
      Distr2\=[],nonvar(Distr2),
      findall(Fr-C2,(on(C1,Distr),on(C2,Distr1),on(C3,Distr2),
                         tds(C1,C2,C3,Fr)),Cs),
                         Cs\=[],
      sort(Cs,Frs),reverse(Frs,[_-Cat2|Rev]), 
      on(Cat1,Mine1),
      matchdistrs(Cat2,Cat1),
      tevalalls([W1-Cat2-Mine1-1,W2-Distr2-Mine2-N2|Disoutss],Diss,Ev),
      !.

tevalalls([W-Distr-Mine-N,W1-Distr1-Mine1-2,W2-Distr2-Mine2-N2|Disoutss],[W-Cat-N0|Diss],[N0|Ev]):-
      selectone(W-Distr-Mine-N,W-Cat-N0,N0),
      is_list(Distr1),
      is_list(Mine1),
      length(Distr1,L1),
      length(Mine1,L2), L2=<L1,
      sort(Mine1,Frs),reverse(Frs,[_-_-_-Cat2|Rev]),
      on(Cat1,Distr1),
      matchdistrs(Cat1,Cat2),
      tevalalls([W1-Distr1-Cat1-1,W2-Distr2-Mine2-N2|Disoutss],Diss,Ev),
      !.
/*
tevalalls([W-Distr-Mine-N,W1-[nil]-Mine1-3,W2-Distr2-Mine2-N2|Disoutss],[W-Cat-N0|Diss],[N0|Ev]):-
      selectone(W-Distr-Mine-N,W-Cat-N0,N0),
      sort(Mine1,Frs),reverse(Frs,[_-_-_-Cat2|Rev]),
      matchdistrs(Cat1,Cat2),
      tevalalls([W1-Cat1-Cat2-1,W2-Distr2-Mine2-N2|Disoutss],Diss,Ev),
      !.
*/
tevalalls([W-Distr-Mine-N,W1-Distr1-Mine1-3,W2-Distr2-Mine2-N2|Disoutss],[W-Cat-N0|Diss],[N0|Ev]):-
      selectone(W-Distr-Mine-N,W-Cat-N0,N0),
      Distr\=[nil],
      is_list(Distr1),
      is_list(Mine1),
      length(Distr1,L1),
      length(Mine1,L2), L1=<L2,N2\=1,
      Distr2\=[],nonvar(Distr2),
      findall(Fr-C2,(on(C1,Distr),on(C2,Distr1),on(C3,Distr2),
                         tds(C1,C2,C3,Fr)),Cs),
                         Cs\=[],
      sort(Cs,Frs),reverse(Frs,[_-Cat2|Rev]), 
      on(_-_-_-Cat1,Mine1),
      matchdistrs(Cat2,Cat1),
      tevalalls([W1-Cat2-Mine1-1,W2-Distr2-Mine2-N2|Disoutss],Diss,Ev),
      !.
tevalalls([W-Distr-Mine-0|Disoutss],[W-Mine-1|Diss],[1|Ev]):-
      (atomic(Mine),A=Mine;Mine=[A]),
      on(Cat,Distr),
      matchdistrs(Cat,A),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-0|Disoutss],[W-Cat-1|Diss],[1|Ev]):-
      (atomic(Distr),A=Distr;Distr=[A]),
      (atomic(Mine),Cat=Mine;Mine=[Cat];is_list(Mine), on(Cat,Mine)),
      A=Cat,
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-0|Disoutss],[W-Mine-1|Diss],[1|Ev]):-
      (atomic(Distr),Cat=Distr;Distr=[Cat]),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-0|Disoutss],[W-Mine-1|Diss],[1|Ev]):-
      (atomic(Mine),Cat=Mine;Mine=[Cat]),
      matchdistrs(Cat1,Cat),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-0|Disoutss],[W-Distr-Mine-0|Diss],[0|Ev]):-
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-1|Disoutss],[W-Mine-1|Diss],[1|Ev]):-
      (atomic(Mine),A=Mine;Mine=[A]),
      on(Cat,Distr),
      matchdistrs(Cat,A),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-1|Disoutss],[W-Cat-1|Diss],[1|Ev]):-
      (atomic(Distr),A=Distr;Distr=[A]),
      (atomic(Mine),Cat=Mine;Mine=[Cat];is_list(Mine), on(Cat,Mine)),
      A=Cat,
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-1|Disoutss],[W-Mine-1|Diss],[1|Ev]):-
      (atomic(Distr),Cat=Distr;Distr=[Cat]),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-1|Disoutss],[W-Mine-1|Diss],[1|Ev]):-
      (atomic(Mine),Cat=Mine;Mine=[Cat]),
      matchdistrs(Cat1,Cat),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-1|Disoutss],[W-Distr-Mine-1|Diss],[0|Ev]):-
      tevalalls(Disoutss,Diss,Ev),
      !.
/*
tevalalls([W-Distr-Mine-1|Disoutss],[W-Cat-1|Diss],[1|Ev]):-
      member(Cat,Mine),
      tevalalls(Disoutss,Diss,Ev),
      !.
*/

tevalalls([W-Distr-Mine-2|Disoutss],[W-A-1|Diss],[1|Ev]):-
      on(Cat,Distr),
      (atomic(Mine),A=Mine;Mine=[A];is_list(Mine), on(A,Mine)),
      matchdistrs(Cat,A),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-2|Disoutss],[W-Mine-1|Diss],[1|Ev]):-
      atomic(Mine),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-2|Disoutss],[W-Distr-1|Diss],[1|Ev]):-
      atomic(Distr),
      tevalalls(Disoutss,Diss,Ev),
      !.
      
tevalalls([W-Distr-Mine-2|Disoutss],[W-Distr-Mine-2|Diss],[0|Ev]):-
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-3|Disoutss],[W-A-1|Diss],[1|Ev]):-
      sort(Mine,Frs),reverse(Frs,[_-_-_-A|Rev]),
      matchdistrs(Cat,A),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-[nil]-Mine-3|Disoutss],[W-Cat-1|Diss],[1|Ev]):-
      (atomic(Mine),Cat=Mine;Mine=[Cat];is_list(Mine), on(Cat,Mine)),
      matchdistrs(Cat1,Cat),
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-3|Disoutss],[W-A-1|Diss],[1|Ev]):-
      on(Cat,Distr),
      on(Prob-_-_-A,Mine),
      matchdistrs(Cat,A),
      tevalalls(Disoutss,Diss,Ev),
      !.
/*
tevalalls([W-Distr-Mine-3|Disoutss],[W-Distr-Mine-3|Diss],[0|Ev]):-
      tevalalls(Disoutss,Diss,Ev),
      !.
tevalalls([W-Distr-Mine-N|Disoutss],Diss,Ev):-
      tevalalls(Disoutss,Diss,Ev),
      !.
*/     


/* punto-2-1*/
disamb0(N1,[W-Distr-Mine-2,W1-Distr1-Mine1-1|Disouts],[W-Distr-Mine0-1|Diss]):-
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      A=punto,
      mtcalltrim(A,'.',W-Mine,B,Mine0),
      disamb2([W1-Distr1-Mine1-1|Disouts],Diss),
      !.      
disamb0(N1,[W-Distr-Mine-2,W1-Distr1-Mine1-1|Disouts],[W-Distr-Mine0-1|Diss]):-
      (atomic(Distr1),B=Distr1;Distr1=[B]),
      A=punto,
      calltrim(A,'.',W-Mine,B,Mine0),
      disamb2([W1-Distr1-Mine1-1|Disouts],Diss),
      !.      

/* punto-2-0*/
disamb0(N1,[W-Distr-Mine-2,W1-Distr1-Mine1-0|Disouts],[W-Distr-Mine0-1|Diss]):-
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      A=punto,
      mtcalltrim(A,'.',W-Mine,B,Mine0),
      disamb2([W1-Distr1-Mine1-0|Disouts],Diss),
      !.      

/* punto-0-2 anche qui il 3 viene posizionato in B */
disamb0(N1,[W-Distr-Mine-0,W1-Distr1-Mine1-2|Disouts],Diss):-
      (atomic(Mine),B=Mine;Mine=[B]),
      A=punto,
      mtcalltrim1(A,'.',B,W,W1-Mine1,Mine0),
      prunelist(Mine0,N,NewMine),
      disamb2([W-Distr-Mine-0,W1-Distr1-NewMine-N|Disouts],Diss),
      !.      

/* punto-1-2*/
disamb0(N1,[W-Distr-Mine-1,W1-Distr1-Mine1-2|Disouts],Diss):-
      (atomic(Mine),B=Mine;Mine=[B]),
      A=punto,
      mtcalltrim1(A,'.',B,W,W1-Mine1,Mine0),
      prunelist(Mine0,N,NewMine),
      disamb2([W-Distr-Mine-1,W1-Distr1-NewMine-N|Disouts],Diss),
      !.      
disamb0(N1,[W-Distr-Mine-1,W1-Distr1-Mine1-2|Disouts],Diss):-
      (atomic(Distr),B=Distr;Distr=[B]),
      A=punto,
      calltrim1(A,'.',B,W,W1-Mine1,Mine0),
      prunelist(Mine0,N,NewMine),
      disamb2([W-Distr-Mine-1,W1-Distr1-NewMine-N|Disouts],Diss),
      !.
     
disamb0(N,Disouts,Diss):-
      disamb2(Disouts,Diss),
      !.      

prunelist(List,N, NewList):-
    (atomic(List), N=1, NewList=List;
      is_list(List),
        subtract(List,[0-_-_-_], Rest),
        (Rest=[A], A\= 0-_-_-_, N=3, NewList=Rest;
                    N=3, NewList=List)),
      !.      

/* 3-2-1 */
disamb31([W-Distr-Mine-3,W2-Distr2-Mine2-2,W1-Distr1-Mine1-1|Disouts],Diss):-
      (atomic(Distr1),B=Distr1;Distr1=[B]),
      calltrim4(Mine,W2-Mine2,W,B,Mine0),
      disamb2([W-Distr-Mine0-1,W2-Distr2-Mine2-2,W1-Distr1-Mine1-1|Disouts],Diss),
      !.      
disamb31([W-Distr-Mine-3,W2-Distr2-Mine2-2,W1-Distr1-Mine1-1|Disouts],Diss):-
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      calltrim4(Mine,W2-Mine2,W,B,Mine0),
      disamb2([W-Distr-Mine0-1,W2-Distr2-Mine2-2,W1-Distr1-Mine1-1|Disouts],Diss),
      !.      

/*  3-2-0 */
disamb31([W-Distr-Mine-3,W2-Distr2-Mine2-2,W1-Distr1-Mine1-0|Disouts],Diss):-
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      calltrim4(Mine,W2-Mine2,W,B,Mine0),
      disamb2([W-Distr-Mine0-1,W2-Distr2-Mine2-2,W1-Distr1-Mine1-0|Disouts],Diss),
      !.      

/*  3-2-2 - qui viene risolto il 3 */
disamb31([W-Distr-Mine-3,W2-Distr2-Mine2-2,W1-Distr1-Mine1-2|Disouts],Diss):-
      calltrim44(Mine,W2-Mine2,Mine1,Mine0),
      disamb2([W-Distr-Mine0-1,W2-Distr2-Mine2-2,W1-Distr1-Mine1-2|Disouts],Diss),
      !.      


disamb2([],[]):-!.
disamb2([W-Distr-Mine-0|[]],[W-Distr-Mine-1|[]]):-
      !.
disamb2([W-Distr-Mine-1|Disouts],[W-Distr-Mine-1|Diss]):-
      Mine=[punt],
      disamb2(Disouts,Diss),
      !.      

/* 3-1-N*/
disamb2([W-Distr-Mine-3,W1-Distr1-Mine1-1,W2-Distr2-Mine2-N|Disouts],Diss):-
      (atomic(Distr1),B=Distr1;Distr1=[B]),
      calltrim3(Mine,W,B,W2-Mine2,Mine0),
      disamb2([W-Distr-Mine0-1,W1-Distr1-Mine1-1,W2-Distr2-Mine2-N|Disouts],Diss),
      !.      
disamb2([W-Distr-Mine-3,W1-Distr1-Mine1-1,W2-Distr2-Mine2-N|Disouts],Diss):-
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      calltrim3(Mine,W,B,W2-Mine2,Mine0),
      disamb2([W-Distr-Mine0-1,W1-Distr1-Mine1-1,W2-Distr2-Mine2-N|Disouts],Diss),
      !.      
disamb2([W-Distr-Mine-3,W1-Distr1-Mine1-0,W2-Distr2-Mine2-N|Disouts],Diss):-
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      calltrim3(Mine,W,B,W2-Mine2,Mine0),
      disamb2([W-Distr-Mine0-1,W1-Distr1-Mine1-0,W2-Distr2-Mine2-N|Disouts],Diss),
      !.      

/* art-2-1*/
disamb2([W-Distr-Mine-2,W1-Distr1-Mine1-1|Disouts],[W-Distr-Mine0-1|Diss]):-
      (atomic(Distr1),B=Distr1;Distr1=[B]),
      Mine=[art|_], A=art,
      filtercats(A,W,B,W1,A1,B1),
      calltrim2(Mine,B1,Mine0),
      disamb2([W1-Distr1-Mine1-0|Disouts],Diss),
      !.      
disamb2([W-Distr-Mine-2,W1-Distr1-Mine1-1|Disouts],[W-Distr-Mine0-1|Diss]):-
      (atomic(Mine1),B=Mine1;Mine1=[B]),
      Mine=[art|_], A=art,
