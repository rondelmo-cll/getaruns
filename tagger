ttgtext(N,W,Pol,Low,W-Cats):-
    (dk(W,Cat)
     ;
     dk(Low,Cat)),
    extractdccats(Cat,Cats),
    !.
ttgtext(N,W,Pol,Low,W-Tag):-
    Pol=0,
    stringof(List,W),
    reverse(List,[V|Rev]),
    (V=e,V1=a;V=e,V1=i;V=a,V1=e;V=i,V1=e;V=i,V1=o;V=o,V1=i),
    reverse([V1|Rev],Lis),
    stringof(Lis,W1),
    lm(W1,Syl,Tags),
    transllmtags(Tags,Tag),
     !.
ttgtext(N,W,Pol,Low,W-Tag):-
    stringof(List,W),
    length(List,L), L=<2,
    concat(W,'_',W1),
    tagtext1(W1,W1-Tag),
     !.
ttgtext(N,W,Pol,Low,W-[num]):-
    stringof([N|List],W),
    name(N,X),
    X >= 48, X =< 57,
     !.
