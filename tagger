     stringof(Lista,W1),
     reverse(Lista,[e,r|Rev]),
     !.
deletezz([],[]):-!.
deletezz([W-Distr-Mine-0|[]],[W-Distr0-Mine-1|[]]):-
      delzz(Distr,Mine,[],Distr0),
      !.
deletezz([W-Distr-Mine-N,W1-Distr1-Mine1-N1|Disouts],[W-Distr-aux-1,W1-Distr1-vppin-1|Diss]):-
      vlm(W,Tag,essere),
      (W1=passato;W1=passati;W1=passata;W1=passate),
      deletezz(Disouts,Diss),
      !.      
deletezz([W-Distr-Mine-1,W1-Distr1-Mine1-2,W2-Distr2-pt-0|Disouts],[W-Distr-Mine-1,W1-Distr1-V-1,W2-Distr2-pt-0|Diss]):-
      atomic(Mine), Mine=nt,
      is_list(Mine1), member(V,Mine1),
      check_vcat(V), checkinfinito(W1),
      deletezz(Disouts,Diss),
      !.      
deletezz([più-Distr-Mine-2,W1-Distr1-Mine1-1|Disouts],[W-Distr0-avv-1,W1-Distr1-Mine1-1|Diss]):-
      (atomic(Mine1),nouncats(Mine1);
        is_list(Mine1),member(Cat,Mine1),nouncats(Cat)),
      deletezz(Disouts,Diss),
      !.      
deletezz([W-Distr-Mine-0,W1-Distr1-Mine1-1|Disouts],[W-Distr0-Mine-1,W1-Distr1-Mine1-1|Diss]):-
      (atomic(Mine1),A=Mine1;Mine1=[A]),
      delzz(Distr,Mine,A,Distr0),
      deletezz(Disouts,Diss),
      !.      
deletezz([W-Distr-Mine-0,W1-Distr1-Mine1-1|Disouts],[W-Distr0-Mine-1,W1-Distr1-Mine1-1|Diss]):-
      \+ atomic(Mine1),Mine1\=[A],
      (atomic(Distr1),A=Distr1;Distr1=[A]),
      delzz(Distr,Distr1,A,Distr0),
      deletezz(Disouts,Diss),
      !.      
deletezz([W-Distr-Mine-N|Disouts],[W-Distr-Mine-N|Diss]):-
      deletezz(Disouts,Diss),
      !.

delzz([Tag|Distr],Mine,Tag,Distr0):-
      (atomic(Mine),A=Mine;Mine=[A]),
      matchdiz(Distr0,Tag,A),
      !.
delzz([Tag|Distr],Mine,Tag,Distr0):-
      (atomic(Mine),A=Mine;Mine=[A]),
      \+ matchdiz(Distr0,Tag,A),
      delzz(Distr,Mine,Tag,Distr0),
      !.
       

