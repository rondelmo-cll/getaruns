    checkonlyfuncs(Funcs,Deps,Diffs),
    !.
checkonlyfuncs([Id-W-Dep-Func|Funcs],[Id-W1-Dep1-Func1,Id1-Clit-Id-Fu|Deps],[Id-W-Dep-Func-Dep1-Func1|Diffs]):-
    c_case(Clit,Cas),
    checkonlyfuncs(Funcs,Deps,Diffs),
    !.
